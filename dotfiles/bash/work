# ==== newsline ====

function gid_send_test() {
  scp $2 gideon@gideon.test.newsline.se:/data/files/incoming/$1/data && gid_manifest gideon@gideon.test.newsline.se $1
}

function gid_manifest() {
  ssh $1 "cd /data/files/incoming/$2/data/ && ls > ../queue/$2_001.txt"
}

function gid_send_acc() {
  scp $2 gideon@gideon.acceptans.newsline.se:/data/files/incoming/$1/data && gid_manifest gideon@gideon.acceptans.newsline.se $1
}

function gid_send() {
  scp $2 gideon@gideon.newsline.se:/data/files/incoming/$1/data && gid_manifest gideon@gideon.newsline.se $1
}

